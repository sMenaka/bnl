<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>BNL</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>FileUploadServlet</display-name>
		<servlet-name>FileUploadServlet</servlet-name>
		<servlet-class>org.birdnestlanka.servlets.Test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileUploadServlet</servlet-name>
		<url-pattern>/uploadFile</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>infoUpload</display-name>
		<servlet-name>infoUpolad</servlet-name>
		<servlet-class>org.birdnestlanka.servlets.InfoServlet</servlet-class>
	</servlet>

	<servlet>

		<servlet-name>update</servlet-name>
		<servlet-class>org.birdnestlanka.servlets.UpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>infoUpolad</servlet-name>
		<url-pattern>/uploadDescrip</url-pattern>
	</servlet-mapping>
	<servlet>

		<servlet-name>delete</servlet-name>
		<servlet-class>org.birdnestlanka.servlets.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
</web-app>